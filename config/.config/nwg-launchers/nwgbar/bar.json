[
  {
    "name": "Lock screen",
    "exec": "swaylock -f -c 000000",
    "icon": "/usr/share/doc/nwg-launchers/examples/icons/system-lock-screen.svg"
  },
  {
    "name": "Logout",
    "exec": "swaymsg exit",
    "icon": "/usr/share/doc/nwg-launchers/examples/icons/system-log-out.svg"
  },
  {
    "name": "Suspend",
    "exec": "systemctl suspend",
    "icon": "/usr/share/doc/nwg-launchers/examples/icons/system-suspend.svg"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "/usr/share/doc/nwg-launchers/examples/icons/system-reboot.svg"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "/usr/share/doc/nwg-launchers/examples/icons/system-shutdown.svg"
  }
]

